{
  "family": "test",
  "executionRoleArn": "arn:aws:iam::148696713809:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "sample-app",
      "image": "148696713809.dkr.ecr.eu-central-1.amazonaws.com/travisdeploytest:45a8611b0f3a8b6f0c558e7a876efd0a3cac83f7",
      "cpu": "512",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "udp"
        }
      ],
      "essential": true,
      "memory": "1024",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/test",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "2048"
}
